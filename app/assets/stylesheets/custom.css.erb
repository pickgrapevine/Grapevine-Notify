body {background: #f1f1f1 !important;}
li.home-page a {text-indent: -9999px; width: 54px; padding: 12px 0px 13px !important;}
#navigation-bar {margin-bottom: 20px !important;}
#slidedeck_frame {top: 20px; width: 930px !important;}
.half {width: 490px; float: left;}
.half_video {width: 460px; float: right; margin-top: 40px;}
.half_video img {position: relative; top: -5px;}
.msg_bg {background: url(<%= 'msg_bg.jpg' %>) no-repeat; height: 92px; width: 980px; margin-bottom: 40px;}
.msg_bg .msg_left {font-size: 36px; color: #333333; font-style: italic; padding: 25px 0 0 20px;}
.msg_bg .msg_right {
    color: #FFFFFF;
    float: right;
    font-size: 30px;
    margin: -40px 30px 0 0;
    text-align: center;
    text-transform: uppercase;
    width: 430px;
}
.msg_bg .msg_right a {color: #fff; text-decoration: none;}
.half p {font-size: 12px; line-height: 24px;}
.half a {color: #663399 !important; text-decoration: underline !important;}
#get-in-touch .via-phone-number h2 {color: #333333 !important;}
#get-in-touch .via-email input {color: #787878;}
a.view-all {color: #b0b0b0 !important; text-decoration: underline;}
a.back-to-top {color: #606060 !important;}
img.right {float: right;}
.slide_bg {width: 980px; margin: 20px auto 0; display: block;}
#footer-extra-wrapper .logo {position: relative; left: -10px;}

#message { margin: 10px 0; padding: 0; }

.error_message { display: block; line-height: 22px; background: #FBE3E4 url(<%= asset_path 'error.gif' %>) no-repeat 10px 6px; padding: 3px 10px 3px 35px; color:#8a1f11;border: 1px solid #FBC2C4; -moz-border-radius:5px; -webkit-border-radius:5px; }